    "electron-builder": "^22.4.0"
  },
  "dependencies": {
    "temp-write": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dtinth/pastebox.git"
  },
  "bugs": {
    "url": "https://github.com/dtinth/pastebox/issues"
  },
  "homepage": "https://github.com/dtinth/pastebox#readme",
  "build": {
    "appId": "me.spacet.pastebox",
